        android:name="com.custom.apidemo.SystemPropertiesSettings" >
    </activity>

    <!-- Reboot -->
    <activity
        android:name="com.custom.apidemo.RebootSettings" >
    </activity>

    <!-- ShutDown -->
    <activity
        android:name="com.custom.apidemo.ShutDownSettings" >
    </activity>

    <!-- Sleep -->
    <activity
        android:name="com.custom.apidemo.SleepSettings" >
    </activity>

    <!-- Screenrecord -->
    <activity
        android:name="com.custom.apidemo.ScreenrecordSettings" >
    </activity>

    <!-- InstallPackage -->
    <activity
        android:name="com.custom.apidemo.InstallPackageSettings" >
